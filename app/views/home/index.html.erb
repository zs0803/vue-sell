<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>home</title>
	<!-- 引入样式 -->
	<link href="http://cdn.bootcss.com/element-ui/1.3.7/theme-default/index.css" rel="stylesheet">
	<link rel="stylesheet" href="/assets/home/index.css">
	<!-- 引入组件库 -->
	<script src="http://cdn.bootcss.com/element-ui/1.3.7/index.js"></script>
</head>
<body>
	<div id="app">
		<div class="auto_table">
			<template>
		    <el-table
		      :data="tableData"
		      style="width: 100%">
		      <el-table-column
		        prop="status"
		        label="规则状态"
		        width="180">
		      </el-table-column>
		      <el-table-column
		        prop="money"
		        label="投资金额"
		        width="180">
		      </el-table-column>
		      <el-table-column
		        prop="ketou"
		        label="可投余额">
		      </el-table-column>
		      <el-table-column
		        prop="term"
		        label="项目期限">
		      </el-table-column>
		      <el-table-column
		        prop="apr"
		        label="年化收益率">
		      </el-table-column>
		      <el-table-column
		        prop="absolute_ranking"
		        label="绝对排名">
		      </el-table-column>
		      <el-table-column
		        prop="effective_ranking"
		        label="有效排名">
		      </el-table-column>
		    </el-table>
		  </template>
		</div>
		<div class="auto_setting">
			<el-button type="primary">设置</el-button>
		</div>
		<el-switch
		  v-model="value"
		  on-color="#13ce66"
		  off-color="#ff4949">
		</el-switch>
	</div>

</body>
<script>
		new Vue({
      el: '#app',
      data: function() {
        return {
          tableData: [{
            status: '关',
            money: '全部',
            ketou: '无限制',
            term: '无限制',
            apr: '无限制',
            absolute_ranking: '123',
            effective_ranking: '1',
          }],
          value: true
        }
      }
    })
	</script>
</html>